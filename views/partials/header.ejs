<!DOCTYPE html>
<html lang="en-US">
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
  <link rel="dns-prefetch" href="//www.google-analytics.com">
  <link rel="dns-prefetch" href="//cdn-images.bokeptube.web.id">
  <meta name="google-site-verification" content="<%= process.env.META_GOOLE_VERIFICATION || 'default-google-verification' %>" />
  <meta name="yandex-verification" content="<%= process.env.META_YANDEX_VERIFICATION || 'default-yandex-verification' %>" />
  <meta name="msvalidate.01" content="<%= process.env.META_MSVALIDATE || 'default-msvalidate' %>" />


<title><%= typeof seo_title !== 'undefined' ? seo_title: current_title %></title>
<meta name="description" content="<%= typeof seo_description !== 'undefined' ? seo_description: current_desc %>">

<meta name="robots" content="<%= typeof no_index !== 'undefined' && no_index ? 'noindex, follow': (typeof robots_meta !== 'undefined' ? robots_meta: 'follow, index, max-snippet:-1, max-video-preview:-1, max-image-preview:large') %>">

<link rel="canonical" href="<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>">
<link rel="shortcut icon" href="<%= typeof favicon_url !== 'undefined' ? favicon_url: '/uploads/favicon.ico' %>" type="image/x-icon">
<link rel="icon" href="<%= typeof favicon_url !== 'undefined' ? favicon_url: '/uploads/favicon.ico' %>" type="image/x-icon">

<link rel="alternate" type="application/rss+xml" title="<%= site_name %> - Video Terbaru" href="/rss">
<link rel="sitemap" type="application/xml" title="Sitemap Utama" href="/sitemap.xml">
<link rel="sitemap" type="application/xml" title="Sitemap Video (Google)" href="/sitemap-video.xml">

<% if (typeof rss_category_slug !== 'undefined' && rss_category_slug) {
%>
<link rel="alternate" type="application/rss+xml"
title="<%= site_name %> - Kategori <%= display_cat %>"
href="/rss/category/<%= rss_category_slug %>">
<%
} %>

<!-- Open Graph Meta Tags -->
<meta property="og:type" content="<%= typeof og_type !== 'undefined' ? og_type: 'website' %>">
<meta property="og:url" content="<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>">
<meta property="og:title" content="<%= typeof seo_title !== 'undefined' ? seo_title: current_title %>">
<meta property="og:description" content="<%= typeof seo_description !== 'undefined' ? seo_description: current_desc %>">
<meta property="og:site_name" content="<%= site_name %>">

<% if (typeof article_tags !== 'undefined' && article_tags.length > 0) {
%>
<% article_tags.forEach(tag => {
%>
<meta property="article:tag" content="<%= tag %>">
<%
}) %>
<%
} %>

<% if (typeof article_section !== 'undefined') {
%>
<meta property="article:section" content="<%= article_section %>">
<%
} %>

<meta property="og:image" content="<%= typeof og_image !== 'undefined' ? og_image: current_image %>">
<meta property="og:image:secure_url" content="<%= typeof og_image !== 'undefined' ? og_image: current_image %>">
<meta property="og:image:width" content="<%= typeof og_image_width !== 'undefined' ? og_image_width: 854 %>">
<meta property="og:image:height" content="<%= typeof og_image_height !== 'undefined' ? og_image_height: 480 %>">
<meta property="og:image:alt" content="<%= typeof seo_title !== 'undefined' ? seo_title.split(' - ')[0]: current_title.split(' - ')[0] %>">
<meta property="og:image:type" content="image/webp">

<% if (typeof og_date !== 'undefined') {
%>
<meta property="article:published_time" content="<%= og_date %>">
<%
} %>

<!-- Twitter Card Meta Tags -->
<meta name="twitter:card" content="<%= typeof twitter_card !== 'undefined' ? twitter_card: 'summary_large_image' %>">
<meta name="twitter:title" content="<%= typeof seo_title !== 'undefined' ? seo_title: current_title %>">
<meta name="twitter:description" content="<%= typeof seo_description !== 'undefined' ? seo_description: current_desc %>">
<meta name="twitter:site" content="<%= typeof twitter_site !== 'undefined' ? twitter_site: '@' + site_name %>">
<meta name="twitter:creator" content="<%= typeof twitter_creator !== 'undefined' ? twitter_creator: '@' + site_name %>">
<meta name="twitter:image" content="<%= typeof twitter_image !== 'undefined' ? twitter_image: current_image %>">
<meta name="twitter:label1" content="Ditulis oleh">
<meta name="twitter:data1" content="<%= typeof schema_author_name !== 'undefined' ? schema_author_name: site_name %>">
<meta name="twitter:label2" content="Waktunya membaca">
<meta name="twitter:data2" content="Kurang dari semenit">

<% if (typeof twitter_player !== 'undefined') {
%>
<meta name="twitter:player" content="<%= twitter_player %>">
<meta name="twitter:player:width" content="1280">
<meta name="twitter:player:height" content="720">
<%
} %>


<% if (typeof schema_publisher_name !== 'undefined') {
%>
<script type="application/ld+json" class="rank-math-schema">
{
"@context": "https://schema.org",
"@graph": [{
"@type": ["Person",
"Organization"],
"@id": "<%= site_url %>/#person",
"name": "<%= schema_publisher_name %>",
"sameAs": "https://twitter.com/<%= site_name %>"
},
{
"@type": "WebSite",
"@id": "<%= site_url %>/#website",
"url": "<%= site_url %>",
"name": "<%= site_name %>",
"alternateName": "<%= site_name %>",
"publisher": {
"@id": "<%= site_url %>/#person"
},
"inLanguage": "id"
},
{
"@type": "ImageObject",
"@id": "<%= typeof og_image !== 'undefined' ? og_image: current_image %>",
"url": "<%= typeof og_image !== 'undefined' ? og_image: current_image %>",
"width": <%= typeof og_image_width !== 'undefined' ? og_image_width: 854 %>,
"height": <%= typeof og_image_height !== 'undefined' ? og_image_height: 480 %>,
"inLanguage": "id"
},
{
"@type": "WebPage",
"@id": "<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>/#webpage",
"url": "<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>",
"name": "<%= typeof seo_title !== 'undefined' ? seo_title: current_title %>",
"datePublished": "<%= typeof schema_date !== 'undefined' ? schema_date: new Date().toISOString().replace('Z', '+07:00') %>",
"dateModified": "<%= typeof schema_date !== 'undefined' ? schema_date: new Date().toISOString().replace('Z', '+07:00') %>",
"isPartOf": {
"@id": "<%= site_url %>/#website"
},
"primaryImageOfPage": {
"@id": "<%= typeof og_image !== 'undefined' ? og_image: current_image %>"
},
"inLanguage": "id"
},
{
"@type": "Person",
"@id": "<%= typeof schema_author_url !== 'undefined' ? schema_author_url: site_url + '/author/' + site_name.toLowerCase() %>",
"name": "<%= typeof schema_author_name !== 'undefined' ? schema_author_name: site_name %>",
"url": "<%= typeof schema_author_url !== 'undefined' ? schema_author_url: site_url + '/author/' + site_name.toLowerCase() %>",
"image": {
"@type": "ImageObject",
"@id": "<%= typeof schema_author_image !== 'undefined' ? schema_author_image: 'https://secure.gravatar.com/avatar/ab04442537d717b73fab19403a00c802db3e20af6389304690fb313b5c0ae3ba?s=96&d=mm&r=g' %>",
"url": "<%= typeof schema_author_image !== 'undefined' ? schema_author_image: 'https://secure.gravatar.com/avatar/ab04442537d717b73fab19403a00c802db3e20af6389304690fb313b5c0ae3ba?s=96&d=mm&r=g' %>",
"caption": "<%= typeof schema_author_name !== 'undefined' ? schema_author_name: site_name %>",
"inLanguage": "id"
},
"sameAs": ["<%= site_url %>"]
},
{
"@type": "BlogPosting",
"headline": "<%= typeof seo_title !== 'undefined' ? seo_title: current_title %>",
"datePublished": "<%= typeof schema_date !== 'undefined' ? schema_date: new Date().toISOString().replace('Z', '+07:00') %>",
"dateModified": "<%= typeof schema_date !== 'undefined' ? schema_date: new Date().toISOString().replace('Z', '+07:00') %>",
"articleSection": "<%= typeof schema_sections !== 'undefined' ? schema_sections.join(', '): 'Bokep Terbaru, Bokep Viral' %>",
"author": {
"@id": "<%= typeof schema_author_url !== 'undefined' ? schema_author_url: site_url + '/author/' + site_name.toLowerCase() %>",
"name": "<%= typeof schema_author_name !== 'undefined' ? schema_author_name: site_name %>"
},
"publisher": {
"@id": "<%= site_url %>/#person"
},
"description": "<%= typeof seo_description !== 'undefined' ? seo_description.replace(/"/g, '&quot;'): current_desc.replace(/"/g, '&quot;') %>",
"name": "<%= typeof seo_title !== 'undefined' ? seo_title: current_title %>",
"@id": "<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>/#richSnippet",
"isPartOf": {
"@id": "<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>/#webpage"
},
"image": {
"@id": "<%= typeof og_image !== 'undefined' ? og_image: current_image %>"
},
"inLanguage": "id",
"mainEntityOfPage": {
"@id": "<%= typeof seo_canonical !== 'undefined' ? seo_canonical: current_url %>/#webpage"
}
}]
}
</script>
<%
} else {
%>
<!-- Fallback Schema untuk halaman non-video -->
<script type="application/ld+json">
{
"@context": "https://schema.org",
"@type": "WebSite",
"name": "<%= site_name %>",
"description": "<%= current_desc %>",
"url": "<%= site_url %>",
"potentialAction": {
"@type": "SearchAction",
"target": "<%= site_url %>/search?q={search_term_string}",
