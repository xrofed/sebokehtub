<script>
// Toggle Mobile Menu
const mobileMenuToggle = document.getElementById('mobile-menu-toggle');
const mobileMenu = document.getElementById('mobile-menu');

mobileMenuToggle.addEventListener('click', function() {
mobileMenu.classList.toggle('hidden');
if (!mobileMenu.classList.contains('hidden')) {
mobileMenu.classList.add('animate-slideDown');
// Sembunyikan search form jika terbuka
mobileSearchForm.classList.add('hidden');
}
});

// Toggle Mobile Search Form
const mobileSearchToggle = document.getElementById('mobile-search-toggle');
const mobileSearchForm = document.getElementById('mobile-search-form');

mobileSearchToggle.addEventListener('click', function() {
mobileSearchForm.classList.toggle('hidden');
if (!mobileSearchForm.classList.contains('hidden')) {
mobileSearchForm.classList.add('animate-slideDown');
// Sembunyikan menu jika terbuka
mobileMenu.classList.add('hidden');
// Focus ke input search
const searchInput = mobileSearchForm.querySelector('input[name="q"]');
if (searchInput) {
searchInput.focus();
}
}
});

// Close menus when clicking outside
document.addEventListener('click', function(event) {
if (!mobileMenu.contains(event.target) && !mobileMenuToggle.contains(event.target)) {
mobileMenu.classList.add('hidden');
}
if (!mobileSearchForm.contains(event.target) && !mobileSearchToggle.contains(event.target)) {
mobileSearchForm.classList.add('hidden');
}
});

// Close menu when clicking Esc key
document.addEventListener('keydown', function(event) {
if (event.key === 'Escape') {
mobileMenu.classList.add('hidden');
mobileSearchForm.classList.add('hidden');
}
});
</script>